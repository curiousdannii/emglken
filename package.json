  "description": "Glk meets Emscripten",
  "author": "Dannii Willis <curiousdannii@gmail.com>",
  "license": "GPL-2.0",
  "keywords": [
    "interactive fiction",
    "interactive-fiction"
  ],
  "repository": "curiousdannii/emglken",
  "bugs": "https://github.com/curiousdannii/emglken/issues",
